{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "250mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "functions": {
    "main.py": {
      "runtime": "python3.11",
      "excludeFiles": [
        "logs/**",
        "**/__pycache__/**",
        ".pytest_cache/**",
        "*.log",
        "docker-compose.yml",
        "Dockerfile",
        ".dockerignore",
        "prometheus.yml",
        "endpoint-scraper/**",
        "main-vercel.py"
      ]
    }
  }
}
